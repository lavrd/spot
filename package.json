{
  "name": "spot-server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "pm2": "pm2 start ./config/pm2.json",
    "node": "NODE_PATH=./src NODE_ENV=development node ./src/app.js",
    "watch": "./node_modules/.bin/esw --watch",
    "lint": "./node_modules/.bin/esw",
    "test": "NODE_PATH=. NODE_ENV=test mocha --recursive --timeout 200000 --exit",
    "migration": "NODE_PATH=./src NODE_ENV=migration node ./src/models/migration.js"
  },
  "dependencies": {
    "connect-mongo": "^2.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongoose": "^4.12.1",
    "morgan": "~1.6.1",
    "nconf": "^0.8.5",
    "serve-favicon": "~2.3.0",
    "telegraf": "^3.17.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "mocha": "^4.0.1",
    "pm2": "^2.7.2",
    "should": "^13.1.2",
    "supertest": "^3.0.0"
  }
}
